{
  "manifest_version": 2,
  "name": "HoldTab",
  "version": "0.0.1",
	"description": "Delays new tab loading until you focus it",
	"author": "Mikhail Vyrtsev",
	"developer": {
		"name": "Mikhail Vyrtsev",
		"url": "https://github.com/Reeywhaar/tab-fast"
	},
	"background":{
		"scripts": [
			"main.js"
		]
	},
	"icons":{
		"128": "icon.png"
	},
	"permissions": [
		"tabs",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	]
}